{
    "manifest_version": 3,
    "name": "Next.js Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension built with Next.js",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"]
  }
  